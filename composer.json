{
    "name": "mortenscheel/laravel-heartbeat",
    "description": "Monitor Laravel queue performance",
    "license": "license",
    "authors": [
        {
            "name": "Morten Scheel",
            "email": "morten@mortenscheel.com",
            "homepage": "https://www.mortenscheel.com"
        }
    ],
    "homepage": "https://github.com/mortenscheel/heartbeat",
    "keywords": ["Laravel", "Heartbeat", "Queue", "Monitor"],
    "require": {
        "illuminate/support": "^8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "MortenScheel\\Heartbeat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MortenScheel\\Heartbeat\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MortenScheel\\Heartbeat\\HeartbeatServiceProvider"
            ]
        }
    }
}
